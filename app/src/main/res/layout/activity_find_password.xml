<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="goBack"
            type="com.msg.gfo_v2.gfo.ui.component.login.FindPasswordActivity" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_tv_find_t"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.048" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_tv_forgot_t"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.214" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_tv_alert_t"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.257" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_et_email_t"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.313" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_et_auth_t"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.452" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_btn_confirm_t"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.413" />
        
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_tv_find_b"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.078" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_tv_forgot_b"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.248" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_tv_alert_b"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.28" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_et_email_b"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.38" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_et_auth_b"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.519" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_btn_confirm_b"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.48" />
        
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_tv_find_s"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.119" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_et_s"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.066" />
        
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_tv_find_e"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.416" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_back_e"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.105" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_tv_forgot_e"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.569" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_tv_alert_e"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.73" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_et_e"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.948" />
        
        
        <TextView
            android:layout_width="107dp"
            android:layout_height="0dp"
            android:text="@string/find_password"
            android:textSize="18sp"
            android:fontFamily="@font/notosans"
            android:textColor="@color/black"
            android:gravity="top"
            android:onClick="@{goBack::onClickPageBtn}"
            app:layout_constraintTop_toTopOf="@id/g_tv_find_t"
            app:layout_constraintBottom_toBottomOf="@id/g_tv_find_b"
            app:layout_constraintStart_toStartOf="@id/g_tv_find_s"
            app:layout_constraintEnd_toEndOf="@id/g_tv_find_e" />
        <TextView
            android:id="@+id/forgot_password"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="@string/write_email"
            android:textSize="18sp"
            android:textColor="@color/black"
            android:fontFamily="@font/notosans"
            app:layout_constraintTop_toTopOf="@id/g_tv_forgot_t"
            app:layout_constraintBottom_toBottomOf="@id/g_tv_forgot_b"
            app:layout_constraintStart_toStartOf="@id/g_et_s"
            app:layout_constraintEnd_toEndOf="@id/g_tv_forgot_e" />
        <TextView
            android:id="@+id/g_tv_alert"
            android:layout_width="239dp"
            android:layout_height="17.5dp"
            android:text="@string/auth_check_alert"
            android:textSize="12sp"
            android:textColor="@color/text_gray"
            android:fontFamily="@font/notosans"
            app:layout_constraintTop_toTopOf="@id/g_tv_alert_t"
            app:layout_constraintBottom_toBottomOf="@id/g_tv_alert_b"
            app:layout_constraintStart_toStartOf="@id/g_et_s"
            app:layout_constraintEnd_toEndOf="@id/g_tv_alert_e" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/arrow_left"
            android:onClick="@{goBack::onClickPageBtn}"
            app:layout_constraintTop_toTopOf="@id/g_tv_find_t"
            app:layout_constraintBottom_toBottomOf="@id/g_tv_find_b"
            app:layout_constraintStart_toStartOf="@id/g_et_s"
            app:layout_constraintEnd_toEndOf="@id/g_back_e"/>
        
        <EditText
            android:id="@+id/et_email"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:hint="@string/et_email"
            android:textSize="14sp"
            android:textColorHint="#999999"
            android:textColor="@color/black"
            android:background="@drawable/bg_edittext"
            android:inputType="textEmailAddress"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            app:layout_constraintTop_toTopOf="@id/g_et_email_t"
            app:layout_constraintBottom_toBottomOf="@id/g_et_email_b"
            app:layout_constraintStart_toStartOf="@id/g_et_s"
            app:layout_constraintEnd_toEndOf="@id/g_et_e" />

        
        <Button
            android:id="@+id/btn_confirm"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="@string/confirm"
            android:textSize="16sp"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            android:fontFamily="@font/notosans"
            android:background="@drawable/btn_login"
            app:layout_constraintTop_toTopOf="@id/g_btn_confirm_t"
            app:layout_constraintBottom_toBottomOf="@id/g_btn_confirm_b"
            app:layout_constraintStart_toStartOf="@id/g_et_s"
            app:layout_constraintEnd_toEndOf="@id/g_et_e" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>